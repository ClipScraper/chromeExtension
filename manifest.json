{
  "manifest_version": 3,
  "name": "My Extension",
  "version": "1.0.0",
  "description": "Displays a popup with bookmarks and more.",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["dist/content.js"],
      "run_at": "document_end"
    }
  ]
}


